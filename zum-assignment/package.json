{
  "name": "zum-assignment",
  "version": "1.0.0",
  "description": "줌인터넷 2022 공채 프론트엔드 개발자 선발을 위한 사전 과제",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "live-server --port=3000",
    "start:server": "json-server --watch db.json --port 8080",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "줌인터넷 프론트엔드 파트",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "json-server": "^0.17.0",
    "live-server": "1.2.1"
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 100
  },
  "homepage": "https://aldrn29.github.com/Zum-Assignment"
}
